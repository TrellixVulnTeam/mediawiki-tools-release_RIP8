        },
        "wdqs": {
            "deployer": "{{ircnick|ryankemper|Ryan}}",
            "what": "...",
            "window": "Wikidata Query Service weekly deploy"
        },
        "no_deploy_day": {
            "deployer": "",
            "what": "No Deploys",
            "window": "No deploys all day! See [[Deployments/Emergencies]] if things are broken."
        }
